from cs50 import get_int

height = 0 
while True:
    height = get_int("height: ")
    if height >= 1 and height <= 8:
        break
        
for i in range(height):
    for j in range(height):
        if (j >= height - i - 1):
            print("#", end="")
        else:
            print(" ", end="")
    print()        
